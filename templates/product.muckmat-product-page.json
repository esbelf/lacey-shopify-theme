    "d0cefaf3-9046-44de-97f1-acdc1f0ee2f7": {
      "type": "video-with-text",
      "blocks": {
        "template--17574336266544__d0cefaf3-9046-44de-97f1-acdc1f0ee2f7-1673532589b29d7680-0": {
          "type": "heading",
          "settings": {
            "heading": "What is a MuckMat® for?"
          }
        },
        "template--17574336266544__d0cefaf3-9046-44de-97f1-acdc1f0ee2f7-1673532589b29d7680-1": {
          "type": "text",
          "settings": {
            "text": "<p>Creates a firm lake bottom<\/p><p>Eliminating aquatic weeds<\/p><p>Prevents sinking into muddy lake bottoms<\/p><p>Building an ideal beach and swim area<\/p>"
          }
        }
      },
      "block_order": [
        "template--17574336266544__d0cefaf3-9046-44de-97f1-acdc1f0ee2f7-1673532589b29d7680-0",
