    "9c0b7a61-dd59-42f8-b006-36d23adf0005": {
      "type": "rich-text",
      "blocks": {
        "template--17574336266544__9c0b7a61-dd59-42f8-b006-36d23adf0005-1673526858daaa97c4-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Benefits of LakeMats®<\/p>",
            "heading_size": "h1"
          }
        },
        "template--17574336266544__9c0b7a61-dd59-42f8-b006-36d23adf0005-1673526858daaa97c4-1": {
          "type": "text",
          "settings": {
            "text": "<p>There are a few reasons why LakeMats® are a great option for your lake or pond area:<\/p>"
          }
        }
      },
      "block_order": [
        "template--17574336266544__9c0b7a61-dd59-42f8-b006-36d23adf0005-1673526858daaa97c4-0",
