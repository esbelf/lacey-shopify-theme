    "6a1a2597-7af2-4b92-9144-9c26aeb5228b": {
      "type": "video-with-text",
      "blocks": {
        "template--17565950083376__6a1a2597-7af2-4b92-9144-9c26aeb5228b-16735280062dc9322d-0": {
          "type": "heading",
          "settings": {
            "heading": "What is a LakeMat® for?"
          }
        },
        "template--17565950083376__6a1a2597-7af2-4b92-9144-9c26aeb5228b-16735280062dc9322d-1": {
          "type": "text",
          "settings": {
            "text": "<ul><li>Elimating lake weeds<\/li><li>Creating a lovely swimming area<\/li><li>Safe for walking lake bottom<\/li><li>No stepping in weeds, rocks or sticks<\/li><\/ul>"
          }
        }
      },
      "block_order": [
        "template--17565950083376__6a1a2597-7af2-4b92-9144-9c26aeb5228b-16735280062dc9322d-0",
