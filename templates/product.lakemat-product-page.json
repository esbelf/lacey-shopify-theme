    "12865129-6d13-4abb-b9ab-9e007613d811": {
      "type": "rich-text",
      "blocks": {
        "template--17565950083376__12865129-6d13-4abb-b9ab-9e007613d811-167352800641a06330-0": {
          "type": "heading",
          "settings": {
            "heading": "<p>Benefits of LakeMats®<\/p>",
            "heading_size": "h1"
          }
        },
        "template--17565950083376__12865129-6d13-4abb-b9ab-9e007613d811-167352800641a06330-1": {
          "type": "text",
          "settings": {
            "text": "<p>There are a few reasons why LakeMats® are a great option for your lake or pond area:<\/p>"
          }
        }
      },
      "block_order": [
        "template--17565950083376__12865129-6d13-4abb-b9ab-9e007613d811-167352800641a06330-0",
