    "c14b25fb-0d14-4e51-badc-09ae2a6248fa": {
      "type": "video-with-text",
      "blocks": {
        "template--17565950083376__c14b25fb-0d14-4e51-badc-09ae2a6248fa-16735280062dc9322d-0": {
